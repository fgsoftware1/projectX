{
  "env": {
    "test": {
      "plugins": [ "istanbul",{
        "useInlineSourceMaps": false,
        "include": [
          "src/**/*.js"
        ],
        "exclude": [
          "**/*.spec.js"
        ]
      }]
    }
  },
  "presets": [
    "es2015",
    "env",
    "@babel/preset-env"
  ],
  "plugins": [
    "karma-babel-preprocessor"
  ]
}